---
export async function getStaticPaths() {
  const data = await fetch("...").then((response) => response.json());

  return data.map((post) => {
    return {
      params: { id: post.id },
      props: { post },
    };
  });
}

//const { id } = Astro.params;
const { post } = Astro.props;
---

<h1>{post.name}</h1>
